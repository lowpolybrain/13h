{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "jsx": "preserve",
    "esModuleInterop": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "strict": true,
    "declaration": false,
    "allowJs": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "target": "esnext",
    "moduleResolution": "node",
    "paths": {
      "@13h/controls": [
        "./lib/controls/src/index.ts"
      ],
      "@13h/core": [
        "./lib/core/src/index.ts"
      ],
      "@13h/filters": [
        "./lib/filters/src/index.ts"
      ],
      "@13h/gimmicks": [
        "./lib/gimmicks/src/index.ts"
      ],
      "@13h/image": [
        "./lib/image/src/index.ts"
      ],
      "@13h/preload": [
        "./lib/preload/src/index.ts"
      ]
    }
  }
}